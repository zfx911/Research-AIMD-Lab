```{r}
resultF09_save = resultF09[!duplicated(resultF09), ]

condition1 <- resultF09_save$BlockName == 'SQ01' & 
              resultF09_save$BookletPart == 1 & 
              resultF09_save$BlockInPart == 2 

condition2 <- resultF09_save$BlockName == 'SQ01' & 
              resultF09_save$BookletPart == 2 & 
              resultF09_save$BlockInPart == 1

combined_condition <- condition1 | condition2

resultF09_save=resultF09_save%>%ungroup()%>% 
  filter(combined_condition)
resultF09_save
