```{r}
resultF05_save = resultF05[!duplicated(resultF05), ]

condition1 <- resultF05_save$BlockName == 'SQ01' & 
              resultF05_save$BookletPart == 1 & 
              resultF05_save$BlockInPart == 2 

condition2 <- resultF05_save$BlockName == 'SQ01' & 
              resultF05_save$BookletPart == 2 & 
              resultF05_save$BlockInPart == 1

combined_condition <- condition1 | condition2

resultF05_save=resultF05_save%>%ungroup()%>% 
  filter(combined_condition)
resultF05_save
