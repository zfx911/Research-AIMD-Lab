resultS04 <- result_total_time_Q4 %>%
  select(idstud, idstud_ori, ImportLogID, BlockName, BookletPart,BlockInPart, module_path, SQ02S04_click_sequence, SQ02S04_wordcount_punc_TA, SQ02S04_wordcount_no_punc_TA, SQ02S04_raw_response_TA,
SQ02S04_wordcount_punc_TB, SQ02S04_wordcount_no_punc_TB, SQ02S04_raw_response_TB, SQ02S04_allsteps_seq, SQ02S04_allsteps_seq_time, SQ02S04_allsteps_timestamp, SQ02S04_total_time)
